  "projectTestCaseTags": [
    {
      "id": 2107193,
      "name": "Solo campos obligatorios",
      "description": "Enviar solo los campos obligatorios con valores válidos, omitiendo todos los opcionales, para probar la solicitud mínima válida."
    },
    {
      "id": 2107194,
      "name": "Semántica válida",
      "description": "Usar valores sintácticamente correctos que también tengan sentido en la lógica de negocio o el contexto del dominio."
    },
    {
      "id": 2107195,
      "name": "Cobertura de combinaciones de enum",
      "description": "Para todos los parámetros de tipo enumeración, aplicar diseño ortogonal para construir sistemáticamente combinaciones de casos de prueba de parámetros y sus valores de enum, con el fin de cubrir los enums."
    },
    {
      "id": 2107196,
      "name": "Otros casos positivos",
      "description": "Proporcionar entradas realistas y válidas que cumplan la especificación del API, produciendo el resultado correcto y esperado."
    },
    {
      "id": 2107197,
      "name": "Campos obligatorios ausentes",
      "description": "Omitir uno o más campos obligatorios en el cuerpo o parámetros de la solicitud."
    },
    {
      "id": 2107198,
      "name": "Valores no válidos",
      "description": "Proporcionar valores con tipo y formato correctos, pero lógicamente no válidos para el campo."
    },
    {
      "id": 2107199,
      "name": "Errores de tipo",
      "description": "Usar valores con tipos de datos incorrectos, como enviar una cadena en lugar de un entero."
    },
    {
      "id": 2107200,
      "name": "Errores de formato",
      "description": "Proporcionar valores con un formato incorrecto, p. ej., patrón de fecha erróneo, dirección de correo mal formada."
    },
    {
      "id": 2107201,
      "name": "Errores semánticos",
      "description": "Usar valores sintáctica y formalmente correctos que sean lógicamente imposibles o violen reglas del dominio."
    },
    {
      "id": 2107202,
      "name": "Otros casos negativos",
      "description": "Proporcionar entradas inválidas o inesperadas que deberían activar el manejo de errores o el rechazo por parte del API."
    },
    {
      "id": 2107203,
      "name": "Nulo",
      "description": "Debe ser el literal JSON `null` (no vacío, no \"null\")."
    },
    {
      "id": 2107204,
      "name": "Cero",
      "description": "Debe ser el literal numérico 0."
    },
    {
      "id": 2107205,
      "name": "Valores vacíos",
      "description": "Debe ser \"\"."
    },
    {
      "id": 2107206,
      "name": "Valores máximos",
      "description": "Proporcionar los valores mínimo y máximo permitidos exactos para campos numéricos o de fecha."
    },
    {
      "id": 2107207,
      "name": "Valores mínimos",
      "description": "Proporcionar los valores mínimo y máximo permitidos exactos para campos numéricos o de fecha."
    },
    {
      "id": 2107208,
      "name": "Desbordamiento",
      "description": "Proporcionar valores justo más allá de los límites mínimo o máximo permitidos."
    },
    {
      "id": 2107209,
      "name": "Subdesbordamiento",
      "description": "Proporcionar valores justo más allá de los límites mínimo o máximo permitidos."
    },
    {
      "id": 2107210,
      "name": "Cadenas demasiado largas",
      "description": "Proporcionar cadenas más largas que la longitud máxima permitida, o más cortas que la longitud mínima requerida."
    },
    {
      "id": 2107211,
      "name": "Cadenas demasiado cortas",
      "description": "Proporcionar cadenas más largas que la longitud máxima permitida, o más cortas que la longitud mínima requerida."
    },
    {
      "id": 2107212,
      "name": "Autorización a nivel de objeto rota",
      "description": "Los usuarios acceden o manipulan objetos de datos sin la autorización adecuada."
    },
    {
      "id": 2107213,
      "name": "Control de acceso",
      "description": "Restringir los recursos y acciones a los que los usuarios pueden acceder según roles, permisos o atributos."
    },
    {
      "id": 2107214,
      "name": "Fallos de autenticación",
